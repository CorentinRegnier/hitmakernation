<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}{{ 'common.title'|trans }}{% endblock %}</title>
    <meta name="description" content="{% block meta_description %}{{ 'common.title'|trans }}{% endblock %}">
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" type="image/ico">

    {# Block de tout les css #}
    {% block stylesheets %}

        {# Block des librairies css externe #}
    {% block stylesheets_lib %}
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    {% endblock %}

        {# Block des librairies css du thème #}
        {% block current_stylesheet %}{% endblock %}
        <link href="{{ asset('css/app.css') }}" rel="stylesheet">

    {% endblock %}

</head>
<body class="page-template-default page page-id-576 pmpro-body-has-access">

<div id="kopa-page-header">


    <div id="kopa-header-top">

        <div class="outer clearfix">

            <div id="logo-image" class="pull-left">
                <a href="https://hitmakernation.com"><img
                            src="https://hitmakernation.com/wp-content/uploads/2017/05/HMN-Transparent.png"
                            alt="HitMakerNation"></a>
            </div>

            <div id="kopa-header-top-inner" class="pull-left">

                <div class="clearfix">

                    <div class="left-col pull-left line-divider clearfix">
                        <ul class="social-links pull-right clearfix">
                            <li>
                                <a href="https://www.facebook.com/HitMakerNation/" target="_blank" title="Facebook"
                                   class="fa fa-facebook"></a>
                            </li>
                            <li>
                                <a href="https://twitter.com/HitMakerNation" target="_blank" title="Twitter"
                                   class="fa fa-twitter">
                                </a>
                            </li>
                            <li>
                                <a href="https://www.youtube.com/channel/UCb99Tach_Xwb0sp75O_fI5A?sub_confirmation=1"
                                   target="_blank" title="YouTube" class="fa fa-youtube"></a>
                            </li>
                        </ul>
                    </div>
                    <div class="right-col line-divider pull-left">

                        <a class="signin-button" href="{{ path('') }}">Sign in</a>
                        <a class="reg-button" href="https://hitmakernation.com/register">Register</a>
                    </div>
                </div>
                <div class="clearfix">
                    <div class="left-col pull-left clearfix">
                        <div class="search-box clearfix">
                            <form action="https://hitmakernation.com/" class="search-form pull-left clearfix"
                                  method="get" _lpchecked="1">
                                <input placeholder="Search..." type="text" value="" name="s" id="s" class="search-text">
                                <button type="submit" class="search-submit"><i class="fa fa-search"></i></button>
                            </form>
                        </div>
                    </div>
                    <div class="right-col pull-left">
                        <a class="shopping-cart-button clearfix" href="https://hitmakernation.com/cart">
                            <i class="fa fa-shopping-cart pull-left"></i>
                            <span class="pull-left">
											0 items - <span class="woocommerce-Price-amount amount">0,00<span
                                            class="woocommerce-Price-currencySymbol">€</span></span>										</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="kopa-header-bottom">
        <div class="outer clearfix">
            <nav id="main-nav">
                <ul id="main-menu" class="clearfix sf-js-enabled sf-arrows">
                    <li id="menu-item-458"
